Subject: [PATCH 5/5] do not install vendored gmock

---
 cmake/install.cmake | 7 -------
 1 file changed, 7 deletions(-)

diff --git a/cmake/install.cmake b/cmake/install.cmake
index eb239b906..7618eaa64 100644
--- a/cmake/install.cmake
+++ b/cmake/install.cmake
@@ -125,10 +125,3 @@ if(protobuf_INSTALL_EXAMPLES)
     DESTINATION "${CMAKE_INSTALL_EXAMPLEDIR}"
     COMPONENT protobuf-examples)
 endif()
-
-if (protobuf_INSTALL_TESTS)
-  install(TARGETS gmock EXPORT protobuf-targets
-    RUNTIME DESTINATION ${CMAKE_INSTALL_BINDIR}
-    LIBRARY DESTINATION ${CMAKE_INSTALL_LIBDIR}
-    ARCHIVE DESTINATION ${CMAKE_INSTALL_LIBDIR})
-endif()
